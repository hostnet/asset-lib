{
    "name":        "hostnet/asset-lib",
    "type":        "hostnet-lib",
    "description": "Tool for finding all dependencies given a set of input files.",
    "license":     "MIT",
    "require": {
        "php":                      ">=7.3",
        "psr/log":                  "^1.0.0",
        "psr/simple-cache":         "^1.0.0",
        "symfony/console":          "^4.0.0 || ^5.0.0",
        "symfony/event-dispatcher": "^4.0.0 || ^5.0.0",
        "symfony/process":          "^4.2.8 || ^5.0.0"
    },
    "require-dev": {
        "hostnet/phpcs-tool":       "^8.3.3",
        "mikey179/vfsstream":       "^1.6.6",
        "phpspec/prophecy-phpunit": "^2.0.1",
        "phpunit/phpunit":          "^9.5.5"
    },
    "autoload": {
        "psr-4": {
            "Hostnet\\Component\\Resolver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hostnet\\Component\\Resolver\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "hostnet/*": true
        }
    }
}
